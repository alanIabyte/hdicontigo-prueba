  },
  "scripts": {
    "start:net": "set HTTPS=true && react-scripts start",
    "start": "set HTTPS=true && react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "testUpdate": "jest --updateSnapshot",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "publish": "npm run testUpdate && git push",
    "publish:stash": "npm run testUpdate && git pull && git push && git stash",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d -dist"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint --fix --no-verify",
      "pre-push": "npm run test --fix"
    }
  },
  "eslintIgnore": [
    "AvisoDePrivacidad.js",
    "TerminosYCondiciones.js",
    "build/*"
  ],
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": [
      "airbnb",
      "airbnb-base",
      "plugin:import/typescript",
      "prettier",
      "prettier/react",
      "plugin:import/errors",
      "plugin:import/warnings"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "max-len": [
            "error",
            130
          ],
          "endOfLine": "auto"
        }
      ],
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "react"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".tsx",
            ".ts",
            "jsx",
            "js"
          ]
        }
      ],
      "import/extensions": "off",
      "no-restricted-syntax": [
        "error",
        "LabeledStatement",
        "WithStatement"
      ],
      "react/prefer-stateless-function": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": false,
          "optionalDependencies": false,
          "peerDependencies": false,
          "packageDir": "./"
        }
      ]
    },
    "parser": "babel-eslint",
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": {
            "resolve": {
              "modules": [
                "node_modules"
              ]
            }
          }
        },
        "node": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ],
          "moduleDirectory": [
            "node_modules",
            "src/"
          ]
        },
        "typescript": {}
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-runtime": "^7.13.9",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.2",
    "@types/react": "^17.0.53",
    "@types/react-dom": "^18.0.11",
    "@types/react-redux": "^7.1.25",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.1",
    "babel-eslint": "^10.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "gh-pages": "^6.1.1",
    "husky": "^4.3.8",
    "jest": "^26.6.0",
    "prettier": "^2.2.1",
    "typescript": "^4.9.5"
  }
}